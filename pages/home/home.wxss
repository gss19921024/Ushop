.page {
    width: 100%;
    height: 100vh;
    position: relative;
}
.page image {
    width: 100%;
    height: 100%;
}
.text {
    position: absolute;
    left: 60rpx;
    bottom: 30rpx;
    width: 100rpx;
    height: 50rpx;
    background: #9d9d9d;
    border-radius: 25rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
}